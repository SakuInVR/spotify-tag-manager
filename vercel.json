{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "VITE_SPOTIFY_CLIENT_ID": "",
    "VITE_SUPABASE_URL": "",
    "VITE_SUPABASE_ANON_KEY": ""
  }
}